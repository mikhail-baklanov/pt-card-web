<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:a="urn:import:ru.relex.webClient.client">
	<ui:style>
	.table {
		font-weight: bold;
		border-style: solid;
		border-width: 1px;
		border-color: gray;
	}
	</ui:style>
	<g:HorizontalPanel width="100%" height="100%">
		<g:cell width="30%">
			<a:AbsentPanel ui:field="absentPanel" styleName="{style.table}">
			</a:AbsentPanel>
		</g:cell> 
		<g:FlowPanel ui:field="logPanel" styleName="{style.table}">
			<a:LogPanel ui:field="cPanel"></a:LogPanel>
		</g:FlowPanel>
	</g:HorizontalPanel>

</ui:UiBinder> 